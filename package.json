{
  "name": "i18next-locize-backend",
  "version": "0.0.1",
  "description": "backend layer for i18next using locize service ",
  "main": "./lib",
  "keywords": [
    "i18next",
    "i18next-backend"
  ],
  "homepage": "https://github.com/i18next/i18next-locize-backend",
  "bugs": "https://github.com/i18next/i18next-locize-backend/issues",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "5.0.0-beta6",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "coveralls": "2.11.6",
    "eslint": "1.10.3",
    "i18next": "2.0.26",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "mversion": "^1.10.1",
    "uglifyify": "3.0.1"
  },
  "scripts": {
    "coverage": "babel-node node_modules/.bin/isparta cover _mocha",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "transpile": "babel src --out-dir lib",
    "browserify": "browserify ./src/index.js --outfile ./bin/index.js -g uglifyify -t babelify -s i18nextIntervalPluralPostProcessor",
    "copy": "cp ./bin/index.js ./i18nextLocizeBackend.min.js",
    "tdd": "mocha test/*.js --compilers js:babel-register --watch",
    "test": "mocha test/*.js --compilers js:babel-register",
    "build": "npm run transpile && npm run browserify && npm run copy",
    "mversion": "mversion",
    "prepublish": "npm test && npm run build",
    "patch": "npm run mversion patch -m",
    "minor": "npm run mversion minor -m",
    "major": "npm run mversion major -m"
  },
  "author": "Jan MÃ¼hlemann <jan.muehlemann@gmail.com> (https://github.com/jamuhl)",
  "license": "MIT"
}
